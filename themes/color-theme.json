{
    "name": "Harmony Dream",
    "type": "dark",
    "colors": {
        "editor.background": "#1E1E2E",
        "editor.foreground": "#D9E0EE",
        "activityBar.background": "#181825",
        "sideBar.background": "#1E1E2E",
        "statusBar.background": "#181825",
        "titleBar.activeBackground": "#181825",
        "tab.activeBackground": "#1E1E2E",
        "tab.inactiveBackground": "#181825",
        "editorGroupHeader.tabsBackground": "#181825",
        "list.activeSelectionBackground": "#313244",
        "list.hoverBackground": "#313244"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#6E738D"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable", "string constant.other.placeholder"],
            "settings": {
                "foreground": "#F5E0DC"
            }
        },
        {
            "name": "Keywords",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#89DCEB"
            }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function", "meta.function-call"],
            "settings": {
                "foreground": "#ABE9B3"
            }
        },
        {
            "name": "Strings",
            "scope": ["string"],
            "settings": {
                "foreground": "#FAE3B0"
            }
        },
        {
            "name": "Numbers",
            "scope": ["constant.numeric"],
            "settings": {
                "foreground": "#F8BD96"
            }
        },
        {
            "name": "Classes",
            "scope": ["entity.name.type", "entity.name.class"],
            "settings": {
                "foreground": "#DDB6F2"
            }
        },
        {
            "name": "Properties",
            "scope": ["variable.other.property"],
            "settings": {
                "foreground": "#96CDFB"
            }
        },
        {
            "name": "Tags",
            "scope": ["entity.name.tag"],
            "settings": {
                "foreground": "#F28FAD"
            }
        }
    ]
} 